}

$textboxUserLogin_KeyDown=[System.Windows.Forms.KeyEventHandler]{
#Event Argument: $_ = [System.Windows.Forms.KeyEventArgs]
	#TODO: Place custom script here
	if ($_.KeyCode -eq "Enter")
	{
		
		if ($textboxUserLogin.Text -ne "")
		{
			$ListGroup = Get-ADPrincipalGroupMembership $textboxUserLogin.Text | Select-Object name
			Update-DataGridView -DataGridView $datagridviewGroups -Item $ListGroup -AutoSizeColumns DisplayedCells
		}
		else
		{
			$textboxUserLogin.Text = "Введи логин Юзера"
		}
		$textboxUserLogin.Text
	}
