$datagridviewGroups_MouseDoubleClick=[System.Windows.Forms.MouseEventHandler]{
#Event Argument: $_ = [System.Windows.Forms.MouseEventArgs]
	#TODO: Place custom script here
	$SelectedGroup = ($datagridviewGroups.CurrentRow.DataBoundItem).name
	
